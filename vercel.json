{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*\\.(js|css|png|jpg|svg|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
